        <xs:element name="Example_Int32" type="xs:int" default="20"/>
        <xs:element name="Example_Color" type="xs:string" default="#ff0000"/>
        <xs:element name="Example_Time" type="xs:int" default="1026"/>
        <xs:element name="Example_Text" type="xs:string" default="Any text content"/>
        <xs:element name="Example_Area" type="xs:string" default=""/>
